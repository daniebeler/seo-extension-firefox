{
  "manifest_version": 3,
  "name": "Black Lemon SEO",
  "version": "1.0",
  "description": "SEO Checker",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
      "default_title": "Black Lemon SEO",
      "default_popup": "index.html",
      "default_icon": {
          "96": "icons/logo-96.png"
      }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}